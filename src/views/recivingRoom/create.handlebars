<script>
 
 $(document).ready(function(){  
   $( "#datepicker" ).datepicker();
   
     $('#myFile2').change(function(){
        $("#frames2").html('');
        for (var i = 0; i < $(this)[0].files.length; i++) {
            $("#frames2").append('<img class="previewFile" src="'+window.URL.createObjectURL(this.files[i])+'" width="300px" height="200px"/>');
        }
    });   
     $('#myFile3').change(function(){
        $("#frames3").html('');
        for (var i = 0; i < $(this)[0].files.length; i++) {
            $("#frames3").append('<img class="previewFile" src="'+window.URL.createObjectURL(this.files[i])+'" width="300px" height="200px"/>');
        }
    });   
});

</script>